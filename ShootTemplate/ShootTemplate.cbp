<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ShootTemplate" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/ShootTemplate" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DPC_VERSION" />
					<Add directory="../SDL2/include" />
				</Compiler>
				<Linker>
					<Add library="../../../../../../Program Files (x86)/CodeBlocks/MinGW/lib/libmingw32.a" />
					<Add library="../SDL2/lib/libSDL2main.a" />
					<Add library="../SDL2/lib/libSDL2.dll.a" />
					<Add library="../SDL2/lib/libSDL2_image.dll.a" />
					<Add directory="../SDL2/lib" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/ShootTemplate" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
